<h2 class="center">Login</h2>
<div class="row">
  <div class="col s8 offset-s2">
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email, "Email Address" %>
      <%= f.text_field :email %>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "Login To Goober" %>
    <% end %>
  </div>
</div>
<p class="center"> Don't have an account? Click <%= link_to 'here', root_path %>
